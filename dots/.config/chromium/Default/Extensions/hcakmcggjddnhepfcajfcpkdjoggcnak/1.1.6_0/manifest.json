{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "content_scripts": [ {
      "js": [ "js/content/verifyinstall.js" ],
      "matches": [ "https://www.uberconference.com/*" ]
   } ],
   "description": "Enables free screen sharing with UberConference",
   "externally_connectable": {
      "matches": [ "https://www.uberconference.com/*" ]
   },
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "UberConference Screen Sharing",
   "permissions": [ "tabs", "desktopCapture", "https://www.uberconference.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.6"
}
