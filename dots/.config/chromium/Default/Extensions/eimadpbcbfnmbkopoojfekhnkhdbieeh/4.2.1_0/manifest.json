{
   "author": "Alexander Shutov",
   "background": {
      "page": "background/index.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "19": "icons/dr_active_19.png",
         "38": "icons/dr_active_38.png"
      },
      "default_popup": "ui/popup/index.html",
      "default_title": "Dark Reader"
   },
   "commands": {
      "addSite": {
         "description": "Toggle current site",
         "suggested_key": {
            "default": "Alt+Shift+A"
         }
      },
      "switchEngine": {
         "description": "Switch filter engine"
      },
      "toggle": {
         "description": "Toggle On/Off",
         "suggested_key": {
            "default": "Alt+Shift+D"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "inject/index.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "Dark theme for every website. Care your eyes, use Dark Reader for night and daily browsing.",
   "icons": {
      "128": "icons/dr_128.png",
      "16": "icons/dr_16.png",
      "48": "icons/dr_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Dark Reader",
   "permissions": [ "fontSettings", "storage", "tabs", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.2.1"
}
