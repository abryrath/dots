{
   "author": "Bartek 'Simek' Kaszubowski",
   "background": {
      "persistance": false,
      "scripts": [ "eventPage.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon19.png",
         "38": "img/icon38.png"
      },
      "default_title": "Toggle XPather"
   },
   "commands": {
      "_execute_browser_action": {
         "description": "Toogle XPather",
         "suggested_key": {
            "default": "Alt+X",
            "linux": "Alt+X",
            "mac": "Alt+X",
            "windows": "Alt+X"
         }
      },
      "input-autocomplete": {
         "description": "Autocomplete XPather input",
         "suggested_key": {
            "default": "Alt+Z",
            "linux": "Alt+Z",
            "mac": "Alt+Z",
            "windows": "Alt+Z"
         }
      },
      "toggle-sidebar": {
         "description": "Toogle XPather sidebar",
         "suggested_key": {
            "default": "Alt+Shift+X",
            "linux": "Alt+Shift+X",
            "mac": "Alt+Shift+X",
            "windows": "Alt+Shift+X"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "css/xpather.css" ],
      "js": [ "lib/jquery-2.1.4.min.js", "lib/jquery.xpath-0.3.1.min.js", "lib/jquery.scrollTo-2.1.1.min.js", "lib/jquery.caret.js", "lib/functions.js", "xpather.constants.js", "xpather.contextMenu.js", "xpather.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Chrome extension for XPath operations on current document, reliable and easy to use with fully featured XPath 2.0 support.",
   "homepage_url": "https://github.com/Simek/XPather",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQCG+PnnMmG6ylPxH8ZtwVlWX9QN+jRvd0pCy9X5V4owNQBANp1yeLzucZR7ODhGSP4JX8ySylJ4ayrRxe5arAKNOkfh12BokDnilgMdxvOXpawx7nncfqeP9XNnkCgiIms0uK7jj1AoWXODmBRVDl4biQTBgThfWaR7Yk151i3bNwIDAQAB",
   "manifest_version": 2,
   "minimum_chrome_version": "22",
   "name": "XPather",
   "offline_enabled": true,
   "permissions": [ "activeTab", "contextMenus", "storage", "\u003Call_urls>" ],
